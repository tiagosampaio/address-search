{
  "name": "tiagosampaio/address-search-php",
  "description": "This is the easiest way to find address by using postcode.",
  "type": "library",
  "keywords": ["sdk", "address search", "php"],
  "license": "OSL-3.0",
  "authors": [
    {
      "name": "Tiago Sampaio",
      "email": "tiago@tiagosampaio.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.0",
    "php-di/php-di": "^6.0",
    "guzzlehttp/guzzle": "^6.3",
    "tiagosampaio/data-object": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4",
    "fzaninotto/faker": "^1.8",
    "squizlabs/php_codesniffer": "^3.3",
    "phpmd/phpmd": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "AddressSearch\\": "src/"
    },
    "files": [
      "src/app/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "AddressSearchTest\\": "dev/test/unit/"
    }
  }
